/* pages/find/find.wxss */
.main
{
    background-color: rgb(245, 239, 239);
    margin: 0;
    padding: 0;
}
.navBar{
    background-color: #fff;
    width: 100%;
    display: flex;
    margin-bottom: 15rpx;
    padding: 30rpx 0rpx;
}
.navBar_item
{
    box-sizing: border-box;
    border-right: 3rpx solid #ebebeb;
    flex: 1;
    text-align: center;

}
.navBar-img image{
    width: 70rpx;
    height: 70rpx;
}

.navBar-title text{
    text-align: center;
    font-size: 29rpx;
    color: #888888;
 }
  

 /* 专题 */
 .content-box{
    border-radius: 10rpx;
    margin: 0 15rpx 15rpx 15rpx;
    overflow: hidden;
 }

 .img image{
    width: 100%;
    display: flex;
    flex-direction: column;
    
 }

 .img-title{
     background-color: #fff;
     font-size: 14px;
     line-height: 100rpx;
     text-align: center;
     /* padding-top: 50rpx; */
 }
 .img-title text{
     color: #fff;
    background-color:#ff6700;
    /* padding: 0 50rpx; */
    font-size: 12px;
    padding:5rpx 15rpx;
    margin-right: 10px;
    border-radius: 10rpx
 }